<launch>
  <arg name="gui" default="false"/>
  <include file="$(find sr_robot_launch)/launch/sr_right_ur10arm_hand.launch">
    <arg name="eth_port" value="_" />
    <arg name="hand_serial" value="650" />
    <arg name="use_moveit" value="true"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="robot_description" value="$(find sr_multi_description)/urdf/right_srhand_ur10_bt_sp_biotacs.urdf.xacro" />
   <arg name="mapping_path" value="$(find sr_edc_launch)/mappings/default_mappings/rh_E_v3.yaml"/>

    <arg name="sim" value="false"/>
  </include>

  <!-- <node name="biotac_republisher" pkg="sr_tactile_sensor_controller" type="biotac_republisher"/> -->
</launch>