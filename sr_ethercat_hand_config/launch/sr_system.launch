<launch>
  <include file="$(find sr_logging_common)/launch/sr_rosbag_log.launch">
    <arg name="log_bag_prefix" value="sr_system"/>
  </include>
  <arg name="sim" default="false"/>
  <arg name="gui" default="true"/>
  <arg name="scene" default="true"/>
  
  <include file="$(find sr_robot_launch)/launch/sr_left_ur10arm_hand.launch">
    <arg name="eth_port" value="enp4s0"/>
    <arg name="hand_serial" value="2346" />
    <arg name="scene" value="$(arg scene)"/>
    <arg name="robot_description" value="$(find sr_multi_description)/urdf/left_srhand_ur10e.urdf.xacro" />
    <arg name="robot_config_file" value="$(find sr_multi_moveit_config)/config/robot_configs/left_sh_ur10e.yaml" />
    <arg name="use_moveit" value="true"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="mapping_path" value="$(find sr_edc_launch)/mappings/default_mappings/lh_E_v3.yaml"/>
    <arg name="sim" value="$(arg sim)"/>
  </include>
</launch>
