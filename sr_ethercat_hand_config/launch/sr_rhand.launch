<launch>
  <include file="$(find sr_edc_launch)/sr_edc.launch" >
    <arg name="config_dir" value="" />
    <arg name="eth_port" value="enp12s0f1" />
    <arg name="hand_serial" value="25" />
    <arg name="hand_id" value="rh" />
    <arg name="robot_description" value="$(find sr_description)/robots/shadowhand_motor.urdf.xacro" />
    
    <arg name="pwm_control" default="false"/>

    <arg name="use_moveit" value="true"/>
    <arg name="sim" value="false"/>
  </include>
</launch>
